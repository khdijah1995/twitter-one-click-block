{
  "manifest_version": 2,
  "name": "Twitter Quick Blocker",
  "description": "Make use of Twitter's blocking features with a simple click",
  "version": "1.0.2",
  "web_accessible_resources" : ["quickblocker.png"],
  "content_scripts": [
    {
      "matches": ["http://twitter.com/*", "https://twitter.com/*"],
      "css": ["quickblocker.css"],
      "js": ["jquery-2.0.3.min.js","quickblocker.js"]
    }
  ]
}